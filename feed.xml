---
layout: null
permalink: /feed.xml
---

<rss version="2.0">
  <channel>
    <title>{{ site.title }}</title>
    <link>{{ site.url }}</link>
    <description>{{ site.description }}</description>
    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>{{ post.url }}</link>
      <description>{{ post.excerpt }}</description>
      <pubDate>{{ post.date | date_to_xmlschema }}</pubDate>
    </item>
    {% endfor %}
  </channel>
</rss>
